# Database
DATABASE_URL=postgres://postgres@localhost:5555/alicia?sslmode=disable

# Backend WebSocket
SERVER_URL=ws://localhost:8090/api/v1/ws

# LLM
LLM_URL=http://localhost:8000/v1
LLM_API_KEY=your-api-key
LLM_MODEL=gpt-4
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSIONS=1536

# MCP Tools (loaded from database, env vars passed to specific servers)
GARDEN_DATABASE_URL=postgres://user:password@localhost:5432/garden?sslmode=disable
KAGI_API_KEY=your-kagi-api-key

# Langfuse (optional - prompts use fallback defaults if not configured)
LANGFUSE_HOST=langfuse.hjkl.lol
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_VERBOSE=false

# OpenTelemetry
OTEL_EXPORTER_OTLP_ENDPOINT=giga:4317
ENVIRONMENT=development

# Memory extraction
MEMORY_EXTRACTION_ENABLED=true
MEMORY_EXTRACTION_TIMEOUT=60s

# Pareto exploration settings
PARETO_MODE=false
PARETO_TARGET_SCORE=3.0
PARETO_MAX_GENERATIONS=5
PARETO_BRANCHES_PER_GEN=3
PARETO_ARCHIVE_SIZE=50
PARETO_ENABLE_CROSSOVER=true
